#ifndef UTILS_H_
#define UTILS_H_

#define TRUE (!0)
#define FALSE (0)
#define SET_PIN(PIN, PORT)	PORT |= (1 << PIN)		// lights on
#define RESET_PIN(PIN, PORT) 	PORT &= ~((1 << PIN)	// lights off
#define TOGGLE_PIN(PIN)

typedef unsigned int uint16;
#endif /* UTILS_H_ */
